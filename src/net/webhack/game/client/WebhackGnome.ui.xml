						<td><img src="str.png" height="40" width="40"></img></td>
						<td><img src="dex.png" height="40" width="40"></img></td>
						<td><img src="con.png" height="40" width="40"></img></td>
						<td><img src="int.png" height="40" width="40"></img></td>
						<td><img src="wis.png" height="40" width="40"></img></td>
						<td><img src="cha.png" height="40" width="40"></img></td>
